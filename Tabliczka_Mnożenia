#include <stdio.h>

int main() zmiana Karola 
{
    printf("Tabliczka mno≈ºenia\n");
    printf("\n");

    int x,y;
    for(x=1; x<11; x++)
    {
        printf("*");
        for(y=1; y<11; y++)
        {
            if (y <= 10 && x == 1)
                printf(" ---%d", x*y);
            else
                printf(" |%2d ", x*y);
        }
        printf("\n");
    }

    return 0;
}
