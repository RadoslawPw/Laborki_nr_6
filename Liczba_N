nowa zmiana
#include <stdio.h>
int main()
{
    int x;

    printf("Podaj ile liczb chcesz zsumować: \n");

    scanf("%d", &x);


    int suma=0;
    int liczba;
    while(x--)
    {
        printf("Podaj %d liczbę: \n", x+1);
        scanf("%d", &liczba);
        suma+=liczba;
    }
    printf("%d", suma);


    return 0;
}

